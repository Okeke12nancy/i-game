{
  "name": "worktest",
  "version": "1.0.0",
  "main": "server-side/index.js",
   "type": "module",
  "scripts": {
    "dev": "concurrently --names \"SERVER,CLIENT\" --prefix-colors \"blue,green\" \"npm run server\" \"npm run client\"",
    "dev-server": "nodemon -r dotenv/config server-side/index.js",
    "start": "node server-side/index.js",
    "server": "npm run dev-server",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "setup-db": "node server-side/scripts/setup-db.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "pg": "^8.16.3",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.10",
    "concurrently": "^9.1.0"
  }
}